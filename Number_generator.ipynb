    "def number_to_add_generator(num):\n",
    "    for i in range(1,num+1):\n",
    "        yield i\n",
    "        \n",
    "n = 0\n",
    "for number in number_to_add_generator(45):\n",
    "    n += number\n",
    "    print(number)\n",
    "print(f\"result of sum all generated number is {n}\")\n",
    "    "
